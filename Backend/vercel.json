{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/teamData",
            "dest": "/api/teams.js"
        },
        {
            "src": "/api/JoinUs",
            "dest": "/api/joinus.js"
        },
        {
            "src": "/api/Collaborate",
            "dest": "/api/collaborate.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js",
            "continue": true
        }
    ]
}