{
    "version": 2,
    "functions": {
        "api/**/*.js": {
            "maxDuration": 60
        }
    },
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/JoinUs",
            "dest": "/api/joinus.js"
        },
        {
            "src": "/Collaborate",
            "dest": "/api/collaborate.js"
        },
        {
            "src": "/teamData",
            "dest": "/api/teamData.js"
        }
    ]
}